{%- for int, int_dict in routed_interfaces_ipv6.items() | sort %}
    interface {{ int }}
    no switchport
    description {{ int_dict.description }}
    {%- for net in int_dict.networks %}
        {%- if net.eui_format_auto %}
            ipv6 address {{ net.adress }}{{ net.mask }} eui-64
        {%- else %}
            ipv6 address {{ net.adress }}{{ net.mask }}
        {%- endif %}
    {% endfor %}
{% endfor %}